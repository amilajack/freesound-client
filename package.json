{
  "name": "freesound-client",
  "version": "0.5.0",
  "main": "./dist/index.js",
  "author": "Amila Welihinda <amilajack@gmail.com>",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@tsconfig/node10": "^1.0.8",
    "@types/jest": "^26.0.24",
    "@types/node": "^14.17.6",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-bliss": "^5.0.0",
    "gh-pages": "^3.2.3",
    "jest": "^26.6.3",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.5.6",
    "typedoc": "^0.21.5",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@types/node-fetch": "^2.5.12",
    "form-data": "^4.0.0",
    "node-fetch": "^2.6.1"
  },
  "scripts": {
    "build": "rimraf dist && tsc",
    "docs:deploy": "gh-pages -d docs && rm -rf docs",
    "docs": "typedoc index.ts --excludeNotExported --excludePrivate --out docs --readme none --mode library",
    "lint": "eslint . --ignore-pattern dist",
    "lint-fix": "yarn lint --fix",
    "test": "yarn build && jest",
    "version": "yarn build && yarn docs && yarn docs:deploy"
  },
  "eslintConfig": {
    "extends": [
      "bliss"
    ]
  },
  "renovate": {
    "extends": [
      "bliss"
    ]
  },
  "keywords": [
    "freesound",
    "node",
    "brower",
    "v2"
  ],
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  }
}
