{
  "name": "freesound-client",
  "version": "0.5.0",
  "main": "./dist/index.js",
  "author": "Amila Welihinda <amilajack@gmail.com>",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@tsconfig/node10": "^1.0.7",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.29",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "eslint": "^7.16.0",
    "eslint-config-bliss": "^5.0.0",
    "gh-pages": "^3.1.0",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.1",
    "typedoc": "^0.17.0-3",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@types/node-fetch": "^2.5.7",
    "form-data": "^3.0.0",
    "node-fetch": "^2.6.1"
  },
  "scripts": {
    "build": "rm -rf dist && tsc",
    "docs:deploy": "gh-pages -d docs && rm -rf docs",
    "docs": "typedoc index.ts --excludeNotExported --excludePrivate --out docs --readme none --mode library",
    "lint": "eslint . --ignore-pattern dist",
    "lint-fix": "yarn lint --fix",
    "test": "yarn build && jest",
    "version": "yarn build && yarn docs && yarn docs:deploy"
  },
  "eslintConfig": {
    "extends": [
      "bliss"
    ]
  },
  "renovate": {
    "extends": [
      "bliss"
    ]
  },
  "keywords": [
    "freesound",
    "node",
    "brower",
    "v2"
  ],
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  }
}
