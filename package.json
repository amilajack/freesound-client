{
  "name": "freesound-client",
  "version": "0.1.0",
  "main": "./lib/index.js",
  "author": "Amila Welihinda <amilajack@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "babel-jest": "^26.1.0",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "eslint": "^7.5.0",
    "eslint-config-bliss": "^5.0.0",
    "jest": "^26.1.0",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "@types/node": "^14.0.25",
    "@types/node-fetch": "^2.5.7",
    "form-data": "^3.0.0",
    "node-fetch": "^2.6.0"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production rm -rf lib && babel index.ts --out-dir lib --extensions \".ts\"",
    "lint": "eslint index.js test",
    "lint-fix": "yarn lint --fix",
    "preversion": "yarn build",
    "test": "yarn build && jest"
  },
  "eslintConfig": {
    "extends": [
      "bliss"
    ]
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": 8
          }
        }
      ],
      "@babel/preset-typescript"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "renovate": {
    "extends": [
      "bliss"
    ]
  },
  "files": [
    "lib"
  ]
}
